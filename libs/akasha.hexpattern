#def(akashaRaw)(
    @vec(-391, 54, -694)
)

#def(akasha)(
    {@vec(-391, 54, -694)} >>
)

#def(akasharead)(
    $akasha
    Jester's Gambit
    Akasha's Distillation
)

#def(akashaSafeRead)(
    $akasharead
    Gemini Decomposition
    Augur's Purification
